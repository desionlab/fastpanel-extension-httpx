{
  "name": "fastpanel-extension-httpx",
  "version": "0.0.1",
  "description": "Extension to work with the \"HTTP\" protocol.",
  "author": "Desionlab <fenixphp@gmail.com>",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "develop": "./node_modules/.bin/tsc --watch",
    "build": "./node_modules/.bin/tsc && npm run test && npm run docs",
    "docs": "typedoc --theme minimal --target 'esnext' --excludeNotExported --excludePrivate --ignoreCompilerErrors --out ./docs/ ./src/",
    "test": "node ./node_modules/mocha/bin/mocha --require ts-node/register  test/**/*.spec.ts",
    "prepare": "./node_modules/.bin/tsc && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desionlab/fastpanel-extension-httpx.git"
  },
  "bugs": {
    "url": "https://github.com/desionlab/fastpanel-extension-httpx/issues"
  },
  "homepage": "https://github.com/desionlab/fastpanel-extension-httpx#readme",
  "keywords": [
    "nodejs",
    "typescript",
    "http",
    "https",
    "express"
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@fastpanel/core": "github:desionlab/fastpanel-core#develop",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.12.12",
    "@types/response-time": "^2.3.3",
    "@types/vorpal": "^1.11.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.1"
  },
  "license": "MIT"
}
